<p id="notice"><%= notice %></p>

<h1>Biomasses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @biomasses.each do |biomass| %>
      <tr>
        <td><%= link_to 'Show', biomass %></td>
        <td><%= link_to 'Edit', edit_biomass_path(biomass) %></td>
        <td><%= link_to 'Destroy', biomass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Biomass', new_biomass_path %>
